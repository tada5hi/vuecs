{
    "name": "@vuecs/list-controls",
    "version": "2.0.0",
    "description": "A package containing list controls.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./core": {
            "types": "./core/index.d.ts",
            "import": "./core/index.mjs",
            "require": "./core/index.cjs"
        },
        "./dist/index.css": "./dist/index.css"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "core",
        "dist"
    ],
    "keywords": [
        "list",
        "list-controls",
        "vue",
        "layout",
        "hyperscript",
        "components",
        "builder",
        "template"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vuecs.git",
        "directory": "packages/list-controls"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production rimraf dist && npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix"
    },
    "devDependencies": {
        "@vuecs/core": "^2.0.0",
        "smob": "^1.4.1",
        "vue": "^3.5.14"
    },
    "peerDependencies": {
        "@vuecs/core": "^2.0.0",
        "smob": "^1.4.1",
        "vue": "^3.5.14"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "08baf626ef03e8db4a732502d795b7a21b3fd1ab"
}

{
    "name": "@vuecs/navigation",
    "version": "2.3.1",
    "description": "A package for multi level navigations.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./core": {
            "types": "./core/index.d.ts",
            "import": "./core/index.mjs",
            "require": "./core/index.cjs"
        },
        "./dist/index.css": "./dist/index.css"
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "core",
        "dist"
    ],
    "keywords": [
        "navigation",
        "navigation-elements",
        "vue",
        "layout",
        "components",
        "navigation",
        "navbar",
        "tier",
        "multi-tier",
        "level",
        "vue-router"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vuecs.git",
        "directory": "packages/navigation"
    },
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production rimraf dist && npm run build:js && npm run build:types",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage"
    },
    "dependencies": {
        "@posva/event-emitter": "^1.0.2"
    },
    "devDependencies": {
        "@vuecs/core": "^2.0.0",
        "@vuecs/link": "^1.0.0",
        "vue": "^3.5.13",
        "vue-router": "^4.4.3"
    },
    "peerDependencies": {
        "@vuecs/core": "^2.0.0",
        "@vuecs/link": "^1.0.0",
        "vue": "^3.x",
        "vue-router": "^4.x"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "08baf626ef03e8db4a732502d795b7a21b3fd1ab"
}

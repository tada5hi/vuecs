{
    "name": "@vuecs/timeago",
    "version": "1.1.0",
    "description": "A package containing a timeago component.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "timeago",
        "time",
        "vue",
        "date"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vuecs.git",
        "directory": "packages/timeago"
    },
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production rimraf dist && npm run build:js && npm run build:types"
    },
    "devDependencies": {
        "date-fns": "^4.1.0",
        "vue": "^3.5.13"
    },
    "peerDependencies": {
        "date-fns": "^3.x || ^4.x",
        "vue": "^3.x"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    }
}
